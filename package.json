{
  "name": "healthCheck",
  "displayName": "Health check",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["*"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "health-time.showConfig",
        "title": "Show Configuration"
      },
      {
        "command": "health-time.showLevelConfig",
        "title": "Configure level"
      },
      {
        "command": "health-time.showAlertTypeConfig",
        "title": "Configure Alert Type" 
      },
      {
        "command": "health-time.showEnableDisableConfig",
        "title": "Enable/Disable Alert"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "health-time" ,
          "title": "Health",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "health-time": [
        {
          "id": "health-time-view",
          "name": "Health View"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
